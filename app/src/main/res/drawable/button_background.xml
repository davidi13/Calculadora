        <shape>
            <corners android:radius="5dp" />
            <solid android:color="@color/hola2" /> <!-- Reemplaza con el color "hola" para el estado presionado -->
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="5dp" />
            <solid android:color="@color/hola" /> <!-- Reemplaza con el color "hola" para el estado normal -->
        </shape>
    </item>
</selector>
